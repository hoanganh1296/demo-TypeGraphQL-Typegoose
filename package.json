{
  "name": "typegraphql-demo",
  "version": "1.0.0",
  "main": "index.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrations:generate": "yarn typeorm-ts-node-esm migration:generate ./src/migrations/update-database -d ./src/utils/mysqlDataSource.ts",
    "migrations:run": "typeorm migration:run -- -d ./src/configs/database.config.ts",
    "lint:fix": "eslint src/*.ts --fix",
    "lint": "eslint src/*.ts",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "npm run lint",
      "git add ."
    ]
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.2.0",
    "apollo-server": "^3.5.0",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "colors": "^1.4.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "graphql": "15.x",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.13",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.30",
    "node-emoji": "^1.11.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.12",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.40",
    "@types/cookie-parser": "^1.4.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.4",
    "@types/mysql": "^2.15.21",
    "@types/node-emoji": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "overrides": {
    "typeorm": {
      "glob": "7.2.3"
    }
  }
}
